    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.periode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.opendate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.closedate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.utilisateur)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.montantRecouvre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.nbrFactures)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.nbrFacturesPayees)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.periode }) |
                @Html.ActionLink("Details", "Details", new { id = item.periode }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.periode })
            </td>
        </tr>
