                    @Html.DisplayFor(modelItem => item.PuissanceDHP)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.nombreSupport)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LineaireBT)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.villages.code_village)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.ID_SERVICE }) |
                    @Html.ActionLink("Details", "Details", new { id = item.ID_SERVICE }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.ID_SERVICE })
                </td>
            </tr>
        }
    </tbody>

</table>


<script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
<script>
    $(document).ready(function () {


        $('#table').DataTable({
            "language": {
