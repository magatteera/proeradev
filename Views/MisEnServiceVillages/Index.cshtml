            </th>
            <th></th>
        </tr>
    </thead>

    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.REGION)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DEPARTEMENT)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.COMMUNE)
                </td>
