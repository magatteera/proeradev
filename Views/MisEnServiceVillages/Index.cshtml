            <th>
                @Html.DisplayNameFor(model => model.PuissanceDHP)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.nombreSupport)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LineaireBT)
            </th>
